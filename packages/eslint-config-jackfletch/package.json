{
  "name": "eslint-config-jackfletch",
  "version": "0.0.1",
  "description": "Jack Fletcher's ESLint config",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "pretests-only": "node ./test/requires",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "prepublish": "(in-install || eslint-find-rules --unused) && (not-in-publish || npm test) && safe-publish-latest",
    "pretest": "npm run --silent lint",
    "test": "npm run --silent tests-only",
    "link:eslint": "cd node_modules/eslint && npm link --production && cd -"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jackfletch/javascript-style"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "jackfletch"
  ],
  "author": "Jack Fletcher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackfletch/javascript-style/issues"
  },
  "homepage": "https://github.com/jackfletch/javascript-style",
  "dependencies": {
    "eslint-config-jackfletch-base": "^14.0.0",
    "object.assign": "^4.1.0",
    "object.entries": "^1.1.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.8.4",
    "babel-preset-airbnb": "^4.4.0",
    "babel-tape-runner": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-find-rules": "^3.4.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "in-publish": "^2.0.0",
    "react": ">= 0.13.0",
    "safe-publish-latest": "^1.1.4",
    "tape": "^5.0.0-next.4"
  },
  "peerDependencies": {
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0"
  },
  "engines": {
    "node": ">= 6"
  }
}
